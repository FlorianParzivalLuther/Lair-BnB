{{! home.hbs }}

{{#each properties}}
  <div class="col-md-4">
    <div class="card">
      <img src="{{this.images.[0]}}" class="card-img-top" alt="Property Image" />
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">{{this.description}}</p>
      </div>
    </div>
  </div>
{{/each}}